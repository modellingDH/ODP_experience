    <Annotation>
        <AnnotationProperty abbreviatedIRI="cp:coversRequirements"/>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">In what ways can one person be engaged in a single activity?</Literal>
    </Annotation>
    <Annotation>
        <AnnotationProperty abbreviatedIRI="cp:hasUnitTest"/>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">PREFIX : &lt;http://modellingdh.github.io/ont/odp/term/&gt;
SELECT DISTINCT ?engagement ?activity WHERE {
